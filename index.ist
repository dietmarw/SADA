item_0 "\n \\item {"

item_1 "\n \\subitem {\\small "
item_01 "\n \\subitem {\\small "
item_x1 "}\n \\subitem {\\small "

item_2 "\n \\subsubitem {\\small "
item_12 "\n \\subsubitem {\\small "
item_x2 "}\n \\subsubitem {\\small "

